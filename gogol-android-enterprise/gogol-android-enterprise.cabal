cabal-version:      2.2
name:               gogol-android-enterprise
version:            0.5.0
synopsis:           Google Play EMM SDK.
homepage:           https://github.com/brendanhay/gogol
bug-reports:        https://github.com/brendanhay/gogol/issues
license:            MPL-2.0
license-file:       LICENSE
author:             Brendan Hay
maintainer:         Brendan Hay <brendan.g.hay+gogol@gmail.com>
copyright:          Copyright (c) 2015-2022 Brendan Hay
category:           Google
build-type:         Simple
extra-source-files:
  README.md
  src/.gitkeep

description:
  Manages the deployment of apps to Android Enterprise devices.
  .
  /Warning:/ This is an experimental prototype/preview release which is still
  under exploratory development and not intended for public use, caveat emptor!
  .
  This library is compatible with version @v1@
  of the API.

source-repository head
  type:     git
  location: git://github.com/brendanhay/gogol.git
  subdir:   gogol-android-enterprise

flag no-field-selectors
  description: If field selectors should be omitted, requires GHC >=9.2
  default:     True

library
  default-language: Haskell2010
  hs-source-dirs:   src gen
  ghc-options:
    -Wall -fwarn-incomplete-uni-patterns
    -fwarn-incomplete-record-updates -funbox-strict-fields

  if (flag(no-field-selectors) && impl(ghc >=9.1))
    default-extensions: NoFieldSelectors

  exposed-modules:
    Network.Google.AndroidEnterprise
    Network.Google.AndroidEnterprise.Devices.ForceReportUpload
    Network.Google.AndroidEnterprise.Devices.Get
    Network.Google.AndroidEnterprise.Devices.GetState
    Network.Google.AndroidEnterprise.Devices.List
    Network.Google.AndroidEnterprise.Devices.SetState
    Network.Google.AndroidEnterprise.Devices.Update
    Network.Google.AndroidEnterprise.Enterprises.AcknowledgeNotificationSet
