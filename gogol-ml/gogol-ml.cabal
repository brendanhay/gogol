cabal-version:      2.2
name:               gogol-ml
version:            0.5.0
synopsis:           Google AI Platform Training & Prediction SDK.
homepage:           https://github.com/brendanhay/gogol
bug-reports:        https://github.com/brendanhay/gogol/issues
license:            MPL-2.0
license-file:       LICENSE
author:             Brendan Hay
maintainer:         Brendan Hay <brendan.g.hay+gogol@gmail.com>
copyright:          Copyright (c) 2015-2022 Brendan Hay
category:           Google
build-type:         Simple
extra-source-files:
  README.md
  src/.gitkeep

description:
  An API to enable creating and using machine learning models.
  .
  /Warning:/ This is an experimental prototype/preview release which is still
  under exploratory development and not intended for public use, caveat emptor!
  .
  This library is compatible with version @v1@
  of the API.

source-repository head
  type:     git
  location: git://github.com/brendanhay/gogol.git
  subdir:   gogol-ml

flag no-field-selectors
  description: If field selectors should be omitted, requires GHC >=9.2
  default:     True

library
  default-language: Haskell2010
  hs-source-dirs:   src gen
  ghc-options:
    -Wall -fwarn-incomplete-uni-patterns
    -fwarn-incomplete-record-updates -funbox-strict-fields

  if (flag(no-field-selectors) && impl(ghc >=9.1))
    default-extensions: NoFieldSelectors

  exposed-modules:
    Network.Google.MachineLearning
    Network.Google.MachineLearning.Ml.Projects.Explain
    Network.Google.MachineLearning.Ml.Projects.GetConfig
    Network.Google.MachineLearning.Ml.Projects.Jobs.Cancel
    Network.Google.MachineLearning.Ml.Projects.Jobs.Create
    Network.Google.MachineLearning.Ml.Projects.Jobs.Get
    Network.Google.MachineLearning.Ml.Projects.Jobs.GetIamPolicy
    Network.Google.MachineLearning.Ml.Projects.Jobs.List
    Network.Google.MachineLearning.Ml.Projects.Jobs.Patch
    Network.Google.MachineLearning.Ml.Projects.Jobs.SetIamPolicy
    Network.Google.MachineLearning.Ml.Projects.Jobs.TestIamPermissions
    Network.Google.MachineLearning.Ml.Projects.Locations.Get
    Network.Google.MachineLearning.Ml.Projects.Locations.List
    Network.Google.MachineLearning.Ml.Projects.Locations.Operations.Cancel
    Network.Google.MachineLearning.Ml.Projects.Locations.Operations.Get
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Create
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Delete
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Get
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.List
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.AddMeasurement
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.CheckEarlyStoppingState
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.Complete
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.Create
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.Delete
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.Get
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.List
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.ListOptimalTrials
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.Stop
    Network.Google.MachineLearning.Ml.Projects.Locations.Studies.Trials.Suggest
    Network.Google.MachineLearning.Ml.Projects.Models.Create
    Network.Google.MachineLearning.Ml.Projects.Models.Delete
    Network.Google.MachineLearning.Ml.Projects.Models.Get
    Network.Google.MachineLearning.Ml.Projects.Models.GetIamPolicy
    Network.Google.MachineLearning.Ml.Projects.Models.List
    Network.Google.MachineLearning.Ml.Projects.Models.Patch
    Network.Google.MachineLearning.Ml.Projects.Models.SetIamPolicy
    Network.Google.MachineLearning.Ml.Projects.Models.TestIamPermissions
    Network.Google.MachineLearning.Ml.Projects.Models.Versions.Create
    Network.Google.MachineLearning.Ml.Projects.Models.Versions.Delete
    Network.Google.MachineLearning.Ml.Projects.Models.Versions.Get
    Network.Google.MachineLearning.Ml.Projects.Models.Versions.List
    Network.Google.MachineLearning.Ml.Projects.Models.Versions.Patch
    Network.Google.MachineLearning.Ml.Projects.Models.Versions.SetDefault
    Network.Google.MachineLearning.Ml.Projects.Operations.Cancel
    Network.Google.MachineLearning.Ml.Projects.Operations.Get
    Network.Google.MachineLearning.Ml.Projects.Operations.List
    Network.Google.MachineLearning.Ml.Projects.Predict
    Network.Google.MachineLearning.Types

  other-modules:
    Network.Google.MachineLearning.Internal.Product
    Network.Google.MachineLearning.Internal.Sum

  build-depends:
    , base        >=4.12  && <5
    , gogol-core  >=0.5.0 && <0.5.1
