cabal-version:      2.2
name:               gogol-youtube-analytics
version:            0.5.0
synopsis:           Google YouTube Analytics SDK.
homepage:           https://github.com/brendanhay/gogol
bug-reports:        https://github.com/brendanhay/gogol/issues
license:            MPL-2.0
license-file:       LICENSE
author:             Brendan Hay
maintainer:         Brendan Hay <brendan.g.hay+gogol@gmail.com>
copyright:          Copyright (c) 2015-2022 Brendan Hay
category:           Google
build-type:         Simple
extra-source-files:
  README.md
  src/.gitkeep

description:
  Retrieves your YouTube Analytics data.
  .
  /Warning:/ This is an experimental prototype/preview release which is still
  under exploratory development and not intended for public use, caveat emptor!
  .
  This library is compatible with version @v2@
  of the API.

source-repository head
  type:     git
  location: git://github.com/brendanhay/gogol.git
  subdir:   gogol-youtube-analytics

flag no-field-selectors
  description: If field selectors should be omitted, requires GHC >=9.2
  default:     True

library
  default-language: Haskell2010
  hs-source-dirs:   src gen
  ghc-options:
    -Wall -fwarn-incomplete-uni-patterns
    -fwarn-incomplete-record-updates -funbox-strict-fields

  if (flag(no-field-selectors) && impl(ghc >=9.1))
    default-extensions: NoFieldSelectors

  exposed-modules:
    Network.Google.YouTubeAnalytics
    Network.Google.YouTubeAnalytics.GroupItems.Delete
    Network.Google.YouTubeAnalytics.GroupItems.Insert
    Network.Google.YouTubeAnalytics.GroupItems.List
    Network.Google.YouTubeAnalytics.Groups.Delete
    Network.Google.YouTubeAnalytics.Groups.Insert
    Network.Google.YouTubeAnalytics.Groups.List
    Network.Google.YouTubeAnalytics.Groups.Update
    Network.Google.YouTubeAnalytics.Reports.Query
    Network.Google.YouTubeAnalytics.Types

  other-modules:
    Network.Google.YouTubeAnalytics.Internal.Product
    Network.Google.YouTubeAnalytics.Internal.Sum

  build-depends:
    , base        >=4.12  && <5
    , gogol-core  >=0.5.0 && <0.5.1
